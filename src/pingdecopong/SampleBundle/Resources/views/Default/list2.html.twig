{% extends "::base.html.twig" %}

{% block title %}list2{% endblock %}



{% block body %}
<h1>list2</h1>
<form action="{{ path('list2') }}" method="get" {{ form_enctype(form) }} novalidate name="list2" id="list2">

    {{ pdp_pager_hidden_render(pager) }}

    <div class="search_box">
        {{ form_widget(form.search) }}
    </div>

    <table>
        <thead>
        {{ pdp_pager_column_render(pager) }}
        </thead>
        <tbody>
        {% for data in datas %}
            <tr>
                <td>{{ data.id }}</td>
                <td>{{ data.name }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {{ pdp_pager_selector_render(pager) }}
    {{ pdp_pager_pagesize_render(pager) }}

</form>
{% endblock %}



{% block javascripts %}

{% endblock %}
